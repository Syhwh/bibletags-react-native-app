{
  "name": "bibletags-react-native-apps",
  "version": "1.0.0",
  "description": "Bible Tags",
  "author": "Andy Hubert",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build-android-beta": "expo build:android --release-channel beta",
    "build-android-production": "expo build:android --release-channel production",
    "build-ios-beta": "expo build:ios --release-channel beta",
    "build-ios-production": "expo build:ios --release-channel production",
    "change-tenant": "./scripts/changeTenant.sh",
    "confirm": "read -p 'Are you sure? ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "ios": "expo start --ios",
    "push-to-beta": "(npm run confirm -s && expo publish --release-channel beta) || true",
    "push-to-production": "(npm run confirm -s && expo publish --release-channel production) || true",
    "usfm-to-sqlite": "node ./scripts/importUsfmToSqlite.js",
    "start": "expo start",
    "translate": "node node_modules/inline-i18n/scripts/makeTranslationFiles.js --dir='./transations' --parse-dirs='src node_modules/bibletags-ui-helper/src' && npm run translate-convert-json-to-csv",
    "translate-convert-json-to-csv": "node node_modules/inline-i18n/scripts/convertTranslationJSONtoCSV.js --dir='./transations'",
    "translate-convert-csv-to-json": "node node_modules/inline-i18n/scripts/convertTranslationCSVtoJSON.js --dir='./transations'"
  },
  "dependencies": {
    "@eva-design/eva": "^1.4.0",
    "@react-native-community/netinfo": "4.6.0",
    "@ui-kitten/components": "^4.4.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "^8.0.0",
    "better-sqlite3": "5.4.3",
    "bibletags-ui-helper": "github:educational-resources-and-services/bibletags-ui-helper#513d8b4f13a7cda12fbdd8301b857196a906db53",
    "bibletags-versification": "github:educational-resources-and-services/bibletags-versification#17e87515d6933edbbcf8b969fa30499ca539f4a9",
    "expo": "^36.0.0",
    "expo-analytics-amplitude": "~8.0.0",
    "expo-asset": "~8.0.0",
    "expo-constants": "~8.0.0",
    "expo-file-system": "~8.0.0",
    "expo-keep-awake": "~8.0.0",
    "expo-localization": "~8.0.0",
    "expo-sqlite": "~8.0.0",
    "expo-store-review": "~2.0.0",
    "find-in-files": "0.5.0",
    "inline-i18n": "github:educational-resources-and-services/inline-i18n#beddb5c27699468712680760c677d4f0f80b8597",
    "moment": "2.24.0",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-circular-progress": "^1.3.6",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-hooks": "^0.9.0",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-simple-side-menu": "github:educational-resources-and-services/react-native-simple-side-menu#22d795ae9000f829365b347c1d8aa82b48c61a30",
    "react-native-svg": "9.13.3",
    "react-redux": "6.0.1",
    "react-router-dom": "5.1.2",
    "react-router-native": "5.1.2",
    "react-use": "13.26.0",
    "reduce-reducers": "1.0.4",
    "redux": "4.0.1",
    "redux-devtools": "3.5.0",
    "redux-persist": "5.10.0"
  }
}
