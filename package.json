{
  "name": "bibletags-react-native-apps",
  "version": "1.0.0",
  "description": "Bible Tags",
  "author": "Andy Hubert",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build-android-beta": "expo build:android --release-channel beta",
    "build-android-production": "expo build:android --release-channel production",
    "build-ios-beta": "expo build:ios --release-channel beta",
    "build-ios-production": "expo build:ios --release-channel production",
    "change-tenant": "./scripts/changeTenant.sh",
    "confirm": "read -p 'Are you sure? ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "ios": "expo start --ios",
    "push-to-beta": "(npm run confirm -s && expo publish --release-channel beta) || true",
    "push-to-production": "(npm run confirm -s && expo publish --release-channel production) || true",
    "setup-translation": "node ./scripts/makeTranslationFiles.js",
    "usfm-to-sqlite": "node ./scripts/importUsfmToSqlite.js",
    "start": "expo start"
  },
  "dependencies": {
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "5.1.1",
    "better-sqlite3": "5.4.0",
    "bibletags-ui-helper": "github:educational-resources-and-services/bibletags-ui-helper#32edc9c3252774314ef327b06c62bfccbff36da8",
    "bibletags-versification": "github:educational-resources-and-services/bibletags-versification#229efd9a5348d105d9e4c49112fe220cdfa97877",
    "expo": "34.0.3",
    "expo-analytics-amplitude": "^6.0.0",
    "expo-asset": "^6.0.0",
    "expo-constants": "^6.0.0",
    "expo-file-system": "^6.0.1",
    "expo-keep-awake": "^6.0.0",
    "expo-sqlite": "^6.0.0",
    "find-in-files": "0.5.0",
    "moment": "2.24.0",
    "native-base": "2.12.1",
    "node-libs-browser": "2.2.0",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-gesture-handler": "1.3.0",
    "react-native-progress": "3.6.0",
    "react-native-reanimated": "1.1.0",
    "react-navigation": "3.9.1",
    "react-redux": "6.0.1",
    "reduce-reducers": "1.0.4",
    "redux": "4.0.1",
    "redux-devtools": "3.5.0",
    "redux-persist": "5.10.0"
  }
}
